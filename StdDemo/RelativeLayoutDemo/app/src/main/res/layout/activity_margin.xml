<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMarginBg">

    <ImageView
        android:id="@+id/Btn_imgback"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerInParent="true"
        android:background="@drawable/myicon"/>
    <ImageView
        android:id="@+id/Btn_imgCancle"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_alignRight="@+id/Btn_imgback"
        android:layout_alignTop="@+id/Btn_imgback"
        android:layout_marginRight="-10dp"
        android:layout_marginTop="-15dp"
        android:background="@drawable/cancel"/>

    <Button
        android:id="@+id/Btn_marginGo"
        android:text="@string/back"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
